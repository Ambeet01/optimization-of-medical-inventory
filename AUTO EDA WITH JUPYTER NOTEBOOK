import matplotlib.pyplot as plt
categories = ['Specialisation 1', 'Specialisation 5', 'Specialisation 3', 'Specialisation 10']
inventory_counts = [150, 200, 120, 180]
plt.pie(inventory_counts, labels=categories, autopct='%1.1f%%', startangle=90, colors=['blue', 'green', 'orange', 'red'])
plt.title('Medical Inventory Pie Chart')
